boolean numeroEncontar(int[]arr,int number){
boolean founded=false;
 for(int i=0;i<arr.length;i++){ 
  if(arr[i]==number){
   founded=true;
   print("numero trobat a la posicio "+i +"  ");
   }
  }
return founded;
}
