isPrime(5);
void isPrime(int n){
if(n>=2){
 if(n==2){
  print("true");
 }else{
  for(int i=2;i<n;i++){
   if(n%n==0 && n%1==0 && n%i==0){
    print("FALSE");
   }
  }
  if(n%n==0&&n%1==0){
   print(" true ");
  }
 }
}else{
 print(" false ");
}
}
isPrime(5);
isPrime(3);
isPrime(2);
