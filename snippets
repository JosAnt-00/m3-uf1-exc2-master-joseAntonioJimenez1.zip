printChars(65,90);
printChars(75,90);
void printChars(int a, int b){
if(a<65||b<65||a>90||b>90){
 print("Error: Enter fora del rang [65-90]");
}else{
for(int i=a;i<=b;i++){
 print((char)i+" ");
}
}
}
printChars(75,90);
printChars(40,90);
printChars(65,90);
void printChars(int a,int... b){
int[]values={65,66,67,68,69,80,81,82,83,84,85,86,87,88,89,90};
 for(int i:b){
 print((char)i);
  }
 }
