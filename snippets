gcd(275,9)
gcd(150,39)
gcd(300,70)
gcd(54,8)
int factorial(int n){
if(n<0){
 return -1;
}
if(n==0){
 return 1;
}else{
 return n*(factorial(n-1));
}
}
factorial(4);
factorial(3);
int[]numbers={1,2,3,4};
getArray(numbers);
int getArray(int[]a){
int arr=0;
for(int i=0;i<a.length;i++){
 arr=i;
}
return arr;
}
